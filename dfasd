[32m+[m[32m            map = (Map) inS.readObject();[m
[32m+[m[32m            inS.close();[m
[32m+[m[32m        }[m
[32m+[m[32m        catch(IOException | ClassNotFoundException e)[m
[32m+[m[32m        {[m
[32m+[m[32m            e.getMessage();[m
[32m+[m[32m        }[m
[32m+[m[32m        return map;[m
[32m+[m[32m    }[m
